config SOC_SPRD
	bool "spred common drivers"
	default n
	help
	  This enables support for common driver in sprd platform.

config NUBIA_KEYBOARD_GPIOKEYS
    tristate "nubia_gpio_keys"
    depends on KEYBOARD_GPIO
    default n
    help
        This enables nubia_gpio_keys

